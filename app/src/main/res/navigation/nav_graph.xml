<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.example.declutteringapp.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment_to_viewPagerFragment"
            app:destination="@id/viewPagerFragment"
            app:popUpTo="@id/splashFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_splashFragment_to_homeFragment2"
            app:destination="@id/homeFragment2"
            app:popUpTo="@id/splashFragment"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/viewPagerFragment"
        android:name="com.example.declutteringapp.ViewPagerFragment"
        android:label="fragment_view_pager"
        tools:layout="@layout/fragment_view_pager" >
        <action
            android:id="@+id/action_viewPagerFragment_to_homeFragment2"
            app:destination="@id/homeFragment2"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_enter_anim"
            app:popEnterAnim="@anim/nav_default_enter_anim"
            app:popExitAnim="@anim/nav_default_exit_anim"
            app:popUpTo="@id/homeFragment2"
            app:popUpToInclusive="false" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment2"
        android:name="com.example.declutteringapp.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment2_to_startFragment2"
            app:destination="@id/startFragment2" />
        <action
            android:id="@+id/action_homeFragment2_to_logInFragment"
            app:destination="@id/logInFragment" />
    </fragment>
    <fragment
        android:id="@+id/thirdFragment"
        android:name="com.example.declutteringapp.ThirdFragment"
        android:label="fragment_third"
        tools:layout="@layout/fragment_third" >
        <action
            android:id="@+id/action_thirdFragment_to_homeFragment2"
            app:destination="@id/homeFragment2" />
    </fragment>
    <fragment
        android:id="@+id/startFragment2"
        android:name="com.example.declutteringapp.StartFragment"
        android:label="fragment_start"
        tools:layout="@layout/fragment_start" >
        <action
            android:id="@+id/action_startFragment2_to_toolsFragment2"
            app:destination="@id/toolsFragment2" />
        <action
            android:id="@+id/action_startFragment2_to_mySpaceFragment2"
            app:destination="@id/mySpaceFragment2" />
        <action
            android:id="@+id/action_startFragment2_to_challengesFragment2"
            app:destination="@id/challengesFragment2" />
        <action
            android:id="@+id/action_startFragment2_to_tipsFragment"
            app:destination="@id/tipsFragment" />
    </fragment>
    <fragment
        android:id="@+id/toolsFragment2"
        android:name="com.example.declutteringapp.ToolsFragment"
        android:label="fragment_tools"
        tools:layout="@layout/fragment_tools" />
    <fragment
        android:id="@+id/profileFragment2"
        android:name="com.example.declutteringapp.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment2_to_logInFragment"
            app:destination="@id/logInFragment" />
    </fragment>
    <fragment
        android:id="@+id/mySpaceFragment2"
        android:name="com.example.declutteringapp.MySpaceFragment"
        android:label="fragment_my_space"
        tools:layout="@layout/fragment_my_space" />
    <fragment
        android:id="@+id/challengesFragment2"
        android:name="com.example.declutteringapp.ChallengesFragment"
        android:label="fragment_challenges"
        tools:layout="@layout/fragment_challenges" />
    <fragment
        android:id="@+id/logInFragment"
        android:name="com.example.declutteringapp.LogInFragment"
        android:label="LogInFragment" >
        <action
            android:id="@+id/action_logInFragment_to_startFragment2"
            app:destination="@id/startFragment2" />
    </fragment>
    <fragment
        android:id="@+id/tipsFragment"
        android:name="com.example.declutteringapp.TipsFragment"
        android:label="fragment_tips"
        tools:layout="@layout/fragment_tips" />

</navigation>